{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
					"build",
					"${workspaceFolder}/Sebir.csproj"
			],
			"problemMatcher": "$msCompile"
		},
		{
			"type": "npm",
			"script": "lint",
			"problemMatcher": {
				"base": "$tslint5",
				"fileLocation": "relative"
			}
		}
	],
}